 ==> 
( ( stmt_typ t_scopes_tup  (  order  ,  funn  ,   (  delta_g  ,  delta_b  ,  delta_x  ,  delta_t  )   )  Prs_n (stmt_app tbl ((MAP (\(e_,tau_,b_) . e_) e_tau_b_list))) )))

