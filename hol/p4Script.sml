   (t_scopes_to_pass f delta_g delta_b t_scope_list_g = SOME t_scope_list_g') /\
   (t_scope' = mk_tscope txdl lol ) /\

	 SOME (a',scope_list', status) = IoE (a,g_scope_list, local_scopes ) /\

